{% extends "MyJobsAdminBundle::layout.html.twig" %}

{% trans_default_domain 'FOSUserBundle' %}

{% block header %}
    MyJobs - Mon Compte - Profil
{% endblock %}

{% block menu %}
    {% include 'MyJobsAdminBundle:Account:menu.html.twig' %}
{% endblock %}

{% block body %}

    <ol class="breadcrumb">
        <li> <a href="{{ path('account_home_page') }}"> Mon Compte </a> </li>
        <li> <a href="{{ path('account_profil') }}"> Profil </a> </li>
        <li class="active"> Editer </li>
    </ol>

    <div class="well">
{{ form_start(form, { 'action': path('account_profil_edit'), 'attr': { 'class': 'fos_user_profile_edit' } }) }}
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'profile.edit.submit'|trans }}" class="btn btn-primary" />
    </div>
{{ form_end(form) }}
    </div>

{% endblock %}